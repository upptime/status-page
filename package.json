    "semantic-release": "^19.0.2",
    "semantic-release-gitmoji": "^1.4.4",
    "svelte": "^3.24.1",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  }
